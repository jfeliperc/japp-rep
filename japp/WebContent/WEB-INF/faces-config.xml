<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<navigation-rule>
		<from-view-id>/*.*</from-view-id>
        
        <navigation-case>
            <from-outcome>login</from-outcome>
            <to-view-id>/pages/login.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>inicio</from-outcome>
            <to-view-id>/pages/inicio.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>relatorio</from-outcome>
            <to-view-id>/pages/inicioRelatorio.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>cadLogin</from-outcome>
            <to-view-id>/pages/cadLogin.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>pessoa</from-outcome>
            <to-view-id>/pages/pessoa.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>produto</from-outcome>
            <to-view-id>/pages/produto.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
            <from-outcome>tipoGrupoProduto</from-outcome>
            <to-view-id>/pages/tipoGrupoProduto.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
        	<from-outcome>empresa</from-outcome>
        	<to-view-id>/pages/empresa.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
        	<from-outcome>cliente</from-outcome>
        	<to-view-id>/pages/cliente.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
        	<from-outcome>fornecedor</from-outcome>
        	<to-view-id>/pages/fornecedor.xhtml</to-view-id>
        </navigation-case> 
        
        <navigation-case>
        	<from-outcome>receita</from-outcome>
        	<to-view-id>/pages/receita.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
        	<from-outcome>estoque</from-outcome>
        	<to-view-id>/pages/estoque.xhtml</to-view-id>
        </navigation-case>
        
        <navigation-case>
        	<from-outcome>acesso</from-outcome>
        	<to-view-id>/pages/acesso.xhtml</to-view-id>
        </navigation-case> 
               
        <navigation-case>
        	<from-outcome>tipoReceita</from-outcome>
        	<to-view-id>/pages/tipoReceita.xhtml</to-view-id>
        </navigation-case> 
           
        <navigation-case>
        	<from-outcome>cardapio</from-outcome>
        	<to-view-id>/pages/cardapio.xhtml</to-view-id>
        </navigation-case>    
            
        <navigation-case>
        	<from-outcome>servico</from-outcome>
        	<to-view-id>/pages/servico.xhtml</to-view-id>
        </navigation-case> 
        
        <navigation-case>
        	<from-outcome>atividade</from-outcome>
        	<to-view-id>/pages/atividade.xhtml</to-view-id>
        </navigation-case>
        
	</navigation-rule>

    <application>
        <resource-bundle>
            <base-name>resources</base-name>
            <var>msgs</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Bundle</base-name>
            <var>bundle</var>
        </resource-bundle>
    </application>
    
    <lifecycle>  
        <phase-listener>com.module.faces.geral.ListenerJapp</phase-listener>  
    </lifecycle> 
        
</faces-config>	